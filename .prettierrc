{
  "trailingComma": "all",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "printWidth": 110,
  "endOfLine": "lf",
  "singleAttributePerLine": true,
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "importOrder": [
    "^@angular/(.*)$",
    "^astro(.*)$",
    "^react$",
    "^react-dom$",
    "^next/(.*)$",
    "^@hono/(.*)$",
    "^hono(.*)$",
    "",
    "^@/(.*)$",
    "",
    "<THIRD_PARTY_MODULES>",
    "",
    "^[./]"
  ]
}
